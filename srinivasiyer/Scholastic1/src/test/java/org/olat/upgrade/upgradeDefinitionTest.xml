<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context" 
	xsi:schemaLocation="
  http://www.springframework.org/schema/beans 
  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd 
  http://www.springframework.org/schema/context 
  http://www.springframework.org/schema/context/spring-context-3.0.xsd">
  
<bean id="olatupgrades" class="org.olat.upgrade.UpgradesDefinitions" >
	<property name="upgrades">
		<list>
			<bean id="upgrade_test" class="org.olat.upgrade.UpgradeTest" >
				<property name="alterDbStatements" value="alter_7_0_x_to_7_1_0.sql"/>
			</bean>
		</list>
	</property>
</bean>

</beans>